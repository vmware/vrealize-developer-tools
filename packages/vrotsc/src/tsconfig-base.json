{
  "compilerOptions": {
    "pretty": true,
    "lib": ["es2015.iterable", "es2015.generator", "es5", "dom"],
    "target": "es5",
    "rootDir": ".",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "noEmitOnError": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "newLine": "lf",
    "typeRoots": ["./node_modules/@types"]
  }
}
